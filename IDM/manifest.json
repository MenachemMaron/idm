{
    "manifest_version": 2,
    "name": "testing",
    "version": "1.0",
    "description": "testing",
    "permissions": [
        "contextMenus",
        "tabs",
        "downloads"
    ],
    "background": {
        "page": "js/background.html"
    },
    "browser_action": {
        "default_icon": "images/icon.jpg"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/content_script.js"]
    }],
    "icons": {
        "16": "images/icon.jpg",
        "32": "images/icon.jpg",
        "48": "images/icon.jpg",
        "128": "images/icon.jpg"
    },
    "web_accessible_resources": [
        "js/content_script.js",
        "js/background.js"
    ]
}